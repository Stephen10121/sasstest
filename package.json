		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.22.0",
		"clsx": "^2.1.1",
		"lucide-svelte": "^0.507.0",
		"mode-watcher": "^1.0.4",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^3.2.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
